{
    "version": "8.5.1",
    "description": "Open platform for analytics and data monitoring",
    "homepage": "https://grafana.com/",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://pd.zwc365.com/https://dl.grafana.com/oss/release/grafana-8.5.1.windows-amd64.zip",
            "hash": "d3c7fd66907fb1afea4de9d4008bc1de9422c054d822bcfe9dea297f51eaffb6"
        }
    },
    "extract_dir": "grafana-8.5.1",
    "bin": [
        [
            "bin\\grafana-server.exe",
            "grafana-server",
            "-homepath \"$dir\""
        ]
    ],
    "persist": [
        "conf",
        "data"
    ],
    "checkver": {
        "url": "https://grafana.com/grafana/download?platform=windows",
